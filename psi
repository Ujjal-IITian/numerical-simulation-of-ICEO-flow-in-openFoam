FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      psi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    elecNorth
    {
	type            fixedValue;
	value           uniform 0.012927425093633;      
    }
   
    elecSouth
    {
	type            fixedValue;
	value           uniform -0.012927425093633;        
    }
    
    cylinder
    {
	type            fixedValue;
	value           uniform 0;
    }

    "wall.*"
    {
	type            zeroGradient;    
    }

    frontAndBack
    {
	type            empty;     
    }    
}

